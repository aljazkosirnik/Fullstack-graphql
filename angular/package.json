{
  "name": "specto-starter-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "startHmr": "ng serve --configuration hmr --port=4400",
    "inOffice": "ng serve --host=0.0.0.0 --port=4500",
    "build-staging": "ng build --configuration=staging --extra-webpack-config webpack.extra.js",
    "build-prod": "ng build --prod --extra-webpack-config webpack.extra.js",
    "bundle-report": "ng build --prod --stats-json --extra-webpack-config extra-webpack.config.js && webpack-bundle-analyzer dist/specto-starter-angular/stats-es2015.json",
    "dev:ssr": "ng run specto-starter-angular:serve-ssr",
    "serve:ssr": "node dist/specto-starter-angular/server/main.js",
    "build-staging-ssr": "ng build --configuration=staging && ng run specto-starter-angular:server:staging",
    "build-prod-ssr": "ng build --prod && ng run specto-starter-angular:server:production",
    "prerender": "ng run specto-starter-angular:prerender",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codegen": "gql-gen"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0-rc.1",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.0.0-rc.1",
    "@angular/compiler": "^11.0.0-rc.1",
    "@angular/core": "^11.0.0-rc.1",
    "@angular/forms": "^11.0.0-rc.1",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^11.0.0-rc.1",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.0.0-rc.1",
    "@angularclass/hmr": "^2.1.3",
    "@apollo/client": "^3.3.10",
    "@nguniversal/express-engine": "^11.1.2",
    "@ngx-translate/core": "^12.1.2",
    "@sentry/browser": "^5.15.5",
    "apollo-angular": "^2.2.0",
    "domino": "^2.1.6",
    "graphql": "^15.5.0",
    "hammerjs": "^2.0.8",
    "ngx-cookie-service": "^3.0.4",
    "ngx-swiper-wrapper": "^9.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.0.0-rc.1",
    "@angular/language-service": "^11.0.0-rc.1",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-apollo-angular": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
